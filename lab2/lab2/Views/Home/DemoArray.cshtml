@using lab2.Models
@model lab2.Models.Club[]
@{
    ViewBag.Title = "DemoArray";
    Layout = "~/Views/Shared/_BasicLayout.cshtml";
}
@if (Model.Length > 0)
{
    <table>
        <thead><tr><th>Product</th><th>Price</th></tr></thead>
        <tbody>

            @foreach (Club p in Model)
            {
                <tr>
                    <td>@p.Name</td>
                    <td>$@p.Price</td>
                </tr>
            }
        </tbody>
    </table> }
else
{
    <h2>No product data</h2>}